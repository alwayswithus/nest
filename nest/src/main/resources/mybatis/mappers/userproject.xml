<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="userproject">
	<select id="selectUserProject" parameterType="long" resultType="userprojectvo">
	<![CDATA[
	 select project_no as projectNo, 
			role_no as roleNo		
	 from userproject
	 where user_no = #{authUserNo}
	]]> 
	</select>
</mapper>